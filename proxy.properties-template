# HTTP(S) proxy server host name
http.proxyHost = my-proxy.mydomain.com
https.proxyHost = my-proxy.mydomain.com
#HTTP(S) proxy server port
http.proxyPort = 8080
https.proxyPort = 8080
# HTTP(S) proxy server user name
http.proxyUser = my-proxy-username
https.proxyUser = my-proxy-username
# HTTP(S) proxy server password
# If this line is left commented, the app will ask for the password.
#http.proxyPassword = my-proxy-password
#https.proxyPassword = my-proxy-password
